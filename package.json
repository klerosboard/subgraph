{
  "name": "klerosboard",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "auth": "graph auth --product hosted-service $ACCESS_TOKEN",
    "build": "graph build",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:gnosis": "mustache config/gnosis.json subgraph.template.yaml > subgraph.yaml",
    "deploy:mainnet": "yarn prepare:mainnet && graph deploy --studio klerosboard-mainnet",
    "deploy:gnosis": "yarn prepare:gnosis && graph deploy --studio klerosboard-gnosis"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.71.2",
    "@graphprotocol/graph-ts": "^0.35.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/mustache": "^4",
    "matchstick-as": "0.5.0"
  },
  "volta": {
    "node": "20.13.1",
    "yarn": "4.2.2"
  }
}
